<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	  
	
	
	<GeneDef ParentName="GeneNoseBase">
		<defName>guy762_Nose_Snout</defName>
		<label>snout</label>
		<description>Carriers of this gene have an elongated, canine-like snout.</description>
		<iconPath>UI/Icons/Genes/Gene_Snout</iconPath>
		<displayCategory>guy762_SWparts_Category</displayCategory>
		<displayOrderInCategory>775</displayOrderInCategory>
		<renderNodeProperties>
		  <li>
			<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
			<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
			<texPath>Pawn/HeadAttachments/bothan/snout</texPath>
			<narrowCrownHorizontalOffset>-0.01</narrowCrownHorizontalOffset>
			<useSkinShader>true</useSkinShader>
			<useRottenColor>true</useRottenColor>
			<colorType>Skin</colorType>
			<parentTagDef>Head</parentTagDef>
			<rotDrawMode>Fresh, Rotting</rotDrawMode>
			<skipFlag>Beard</skipFlag>
			<visibleFacing>
			  <li>East</li>
			  <li>South</li>
			  <li>West</li>
			</visibleFacing>
			<drawData>
			  <defaultData>
				<layer>66</layer>
			  </defaultData>
			</drawData>
		  </li>
		</renderNodeProperties>
	  </GeneDef>
	  
	  
	  
