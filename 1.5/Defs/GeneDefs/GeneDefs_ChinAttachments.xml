<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	  
	  
	  <GeneDef>
        <defName>guy762_Beard_chinspines</defName>
        <label>chin spines</label>
        <description>Carriers of this gene grow flexible spines on their chins.</description>
        <iconPath>UI/Icons/Genes/Gene_ChinSpines</iconPath>
        <displayCategory>guy762_SWparts_Category</displayCategory>
        <iconColor>(0.75, 0.75, 0.75)</iconColor>
        <displayOrderInCategory>770</displayOrderInCategory>
        <biostatCpx>0</biostatCpx>
		<renderNodeProperties>
		  <li>
			<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
			<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
			<texPath>Pawn/HeadAttachments/weequay/ChinSpines</texPath>
			<color>(255, 255, 255)</color>
			<narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset>
			<parentTagDef>Head</parentTagDef>
			<skipFlag>Beard</skipFlag>
			<visibleFacing>
			  <li>East</li>
			  <li>South</li>
			  <li>West</li>
			</visibleFacing>
			<drawData>
			  <defaultData>
				<layer>65</layer>
			  </defaultData>
			</drawData>
		  </li>
		</renderNodeProperties>
    </GeneDef>
	
