		<description>Carriers of this gene grow many smaller head tails protruding from the back and sides of the head.</description>
		<displayOrderInCategory>761</displayOrderInCategory>
		<displayCategory>guy762_SWparts_Category</displayCategory>
		<iconPath>UI/Icons/Genes/Gene_HeadboneNautolan</iconPath>
		<renderNodeProperties>
		  <li>
			<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
			<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
			<texPath>Pawn/HeadAttachments/feeorin/nautolan</texPath>
			<parentTagDef>Head</parentTagDef>
			<colorType>Skin</colorType>
			<useRottenColor>true</useRottenColor>
			<useSkinShader>true</useSkinShader>
