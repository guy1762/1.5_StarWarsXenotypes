<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<GeneDef ParentName="GeneEarsBase">
		<defName>guy762_Ears_big</defName>
		<label>big ears</label>
		<description>Carriers of this gene will grow larger-than-usual ears.</description>
		<iconPath>UI/Icons/Genes/Gene_BigEar</iconPath>
		<displayCategory>guy762_SWparts_Category</displayCategory>
		<renderNodeProperties>
		  <li>
			<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
			<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
			<texPath>Pawn/HeadAttachments/sullustan/BigEars</texPath>
			<colorType>Skin</colorType>
			<parentTagDef>Head</parentTagDef>
			<useSkinShader>true</useSkinShader>
			<useRottenColor>true</useRottenColor>
			<rotDrawMode>Fresh, Rotting</rotDrawMode>
			<skipFlag>Hair</skipFlag>
			<drawData>
			  <defaultData>
				<layer>70</layer>
			  </defaultData>
			</drawData>
		  </li>
		</renderNodeProperties>
	</GeneDef>
	
	<GeneDef ParentName="GeneEarsBase">
